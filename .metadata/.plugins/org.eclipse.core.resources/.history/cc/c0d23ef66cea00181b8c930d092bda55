import java.io.*;

public class FileOutputStream {

	public static void main(String[] args) {
		int i;
		String f="D:\\Workspace\\Files\\name1.txt";
		FileOutputStream fout=new FileOutputStream(f, true);
		String s="TATA";
		char ch[]=s.toCharArray();
		for (int i = 0; i < s.length; i++) {
			fout.write(ch[i]);
		}
		fout.close();
		

	}

}
