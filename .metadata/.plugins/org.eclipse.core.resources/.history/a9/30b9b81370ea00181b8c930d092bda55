import java.io.*;

public class FileInputStream {

	public static void main(String[] args) throws IOException {
		
		java.io.FileInputStream fin;
		fin = new java.io.FileInputStream("D:\\Workspace\\Files\\name1.txt");
		
		do {
			int i=fin.read();
			if(i!=-1)
				System.out.println("");
		} while (i!=-1);

	}

}
